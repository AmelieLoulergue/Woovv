<div class="wrapper">
  <h2 class="title-su">Log in</h2>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div>
    <div class="field">
            <div class="field container-su">
              <%= f.email_field :email, autofocus: true, placeholder: "email",class: 'field-su' %>
            </div>
      </div>
    <div class="field container-su">
        <%= f.password_field :password, placeholder: "password" , class:'field-su'%>
    </div>
    <% if devise_mapping.rememberable? %>
      <div class="field container-su remember">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>
        </div>

    <div class="actions div-su">
      <%= f.submit "Log in", class: 'btn-su' %>
    </div>
  <%= render "devise/shared/links" %>

  <% end %>
</div>
