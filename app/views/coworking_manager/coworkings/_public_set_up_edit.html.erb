  <%= form_with model:@public_set_up, url: coworking_manager_coworking_path(@coworking.id, :bike_storage, :coffee_access), method: :patch, local: true do |form| %>
  <div class="edit-coworking">
    <div class="radio">
      <p>Café :</p>
      <div>
        <%= form.label :coffee_access, "Absent" %>
        <%= radio_button("public_set_up", "coffee_access_id","1") %>
      </div>
      <div>
        <%= form.label :coffee_access, "Présent - gratuit" %>
        <%= radio_button("public_set_up", "coffee_access_id","2") %>
      </div>
      <div>
        <%= form.label :coffee_access, "Présent - payant" %>
        <%= radio_button("public_set_up", "coffee_access_id","3") %>
      </div>
    </div>
    <div class="radio">
      <p>Parking vélo sécurisé :</p>
      <div>
        <%= form.label :bike_storage, "Absent" %>
        <%= radio_button("public_set_up","bike_storage_id","1") %>
      </div>
      <div>
        <%= form.label :bike_storage, "Présent - gratuit" %>
        <%= radio_button("public_set_up","bike_storage_id","2") %>
      </div>
      <div>
        <%= form.label :bike_storage, "Présent - payant" %>
        <%= radio_button("public_set_up","bike_storage_id","3") %>
      </div>
    </div>
    <div class="edit-coworking checkbox reverse">
      <div>
        <%= form.label :is_pet_friendly, "Animaux acceptés" %>
        <%= form.check_box :is_pet_friendly, class:"btn btn-secondary active",value: @public_set_up.is_pet_friendly %>
      </div>
    </div>
    <div>
      <%= form.label :network_speed, "Vitesse du réseau :" %>
      <div class = "network">
        <%= form.number_field :network_speed, value: @public_set_up.network_speed %> Mbps
      </div>
    </div>
    <%= form.submit "Enregistrer", class:"btn btn-primary"%>
  </div>
<% end %>
